                x = 1;
            }else{
                x = 2;
            }
            
            return 0;
        }
}

class B extends A{
    boolean y;
    int x;
    public int check(){
        int[] D;
        y = false;
        D = new int[10];
        while(1<0){
            x = new B().check();
            x = x + 1;
            x = D[(D[1])];
        }
        return 4;
        
    }
}


