class Check{
    public static void main(String[] a){
        System.out.println(new A().check());
    }
}

class A{
        int x;
        public int check(){
            int a[];
            x=0;
            if(x<0){
                x = 1;
            }else{
                x = 2;
            }
            return 0;
        }
}

class B extends A{
    boolean y;
    public int check(){
        int[] D;
        y = false;
        D = new int[10];
        while(1<0){
            x = new B().check();
            x = x + 1;
            x = D[(D[1])];
        }
        return 4;
        
    }
}

class C extends B{
    public int check(){
        A x;
        B y;
        C z; 
        //z = this.check();
        y = new B();
        z = new D().trial(this); 
        return 1;
    }
}


class D{
    public C trial(B x){
        C r;
        r = new C();
        return r;
    }
}
